$regular = 400
$light = 300
$medium = 500
$bold = 700

// Temporary fix while waiting for the font pack
// When we get the font pack, move this into common.styl
example-container
modal-view
	font-family "Helvetica Neue"
	font-weight $regular
		
button
tab-button-group
form
	font-family "Helvetica Neue"
	font-weight $medium
	
	& h1
	& h2
	& h3
	& h4
		font-family inherit
		font-weight $regular
		


