#header
	background-color #000
	color white
	background-image url('/images/logo.png')
	background-repeat no-repeat
	background-size 89px 53px
	background-position 30px center

header-title
	@extend p
	color inherit
	display block
	line-height 96px
	text-align right
	padding-right 30px
	
page-wrapper
	display block
	min-height 100%
	margin-bottom -97px
	padding-bottom 96px
	
footer
	@extend #header
	display block
	height 96px